<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<h1>Log in</h1>
<%= form_for(:session,url: login_path) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %> <br/>
  <%= f.label :password %> 
  <%= f.password_field :password %> <br/>
  <%= f.submit "Log in" %>
<% end %>

<p> New user? <%= link_to "Sign up now!",signup_path %></p>
